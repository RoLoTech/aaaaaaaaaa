<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox stylesheets="@Restaurant.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="labTic.ui.RestaurantController">
   <children>
      <AnchorPane prefHeight="400.0" prefWidth="600.0" styleClass="back">
         <children>
            <HBox alignment="CENTER" layoutX="80.0" layoutY="100.0" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="100.0">
               <children>
                  <Button contentDisplay="CENTER" mnemonicParsing="false" onMouseClicked="#btnPrev" style="-fx-background-radius: 25; -fx-min-width: 30; -fx-min-height: 30; -fx-max-width: 30; -fx-max-height: 30;" styleClass="gridpane" text="&lt;" textAlignment="CENTER">
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                     <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </HBox.margin>
                  </Button>
                  <VBox HBox.hgrow="ALWAYS">
                     <children>
                        <HBox prefHeight="100.0" prefWidth="200.0" styleClass="gridpane">
                           <children>
                              <ChoiceBox fx:id="cbLocation" onMouseClicked="#cbLocationClick" prefWidth="150.0" style="-fx-background-color: white; -fx-border-color: white;">
                                 <HBox.margin>
                                    <Insets />
                                 </HBox.margin>
                              </ChoiceBox>
                              <Button mnemonicParsing="false" onMouseClicked="#btnFiltrar" style="-fx-background-color: red;" text="Filtrar">
                                 <cursor>
                                    <Cursor fx:constant="HAND" />
                                 </cursor>
                              </Button>
                           </children>
                           <VBox.margin>
                              <Insets bottom="5.0" />
                           </VBox.margin>
                        </HBox>
                        <GridPane depthTest="DISABLE" prefHeight="400.0" prefWidth="200.0" styleClass="gridpane" stylesheets="@Restaurant.css">
                          <columnConstraints>
                            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                          </columnConstraints>
                          <rowConstraints>
                            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                          </rowConstraints>
                           <children>
                              <VBox onMouseClicked="#cardRestaurant4" prefHeight="200.0" prefWidth="100.0" style="-fx-background-color: white;" GridPane.columnIndex="1" GridPane.rowIndex="1">
                                 <children>
                                    <Text fx:id="text4" strokeType="OUTSIDE" strokeWidth="0.0" text="Restaurant1" />
                                 </children>
                                 <GridPane.margin>
                                    <Insets bottom="10.0" left="5.0" right="10.0" top="5.0" />
                                 </GridPane.margin>
                              </VBox>
                              <VBox onMouseClicked="#cardRestaurant3" prefHeight="200.0" prefWidth="100.0" style="-fx-background-color: white;" GridPane.rowIndex="1">
                                 <children>
                                    <Text fx:id="text3" strokeType="OUTSIDE" strokeWidth="0.0" text="Restaurant3" />
                                 </children>
                                 <GridPane.margin>
                                    <Insets bottom="10.0" left="10.0" right="5.0" top="5.0" />
                                 </GridPane.margin>
                                 <padding>
                                    <Insets right="5.0" />
                                 </padding>
                              </VBox>
                              <VBox onMouseClicked="#cardRestaurant2" prefHeight="200.0" prefWidth="100.0" style="-fx-background-color: white;" GridPane.columnIndex="1">
                                 <children>
                                    <Text fx:id="text2" strokeType="OUTSIDE" strokeWidth="0.0" text="Restaurant2" />
                                 </children>
                                 <GridPane.margin>
                                    <Insets bottom="5.0" left="5.0" right="10.0" top="10.0" />
                                 </GridPane.margin>
                              </VBox>
                              <VBox onMouseClicked="#cardRestaurant1" prefHeight="200.0" prefWidth="100.0" style="-fx-background-color: white;">
                                 <children>
                                    <HBox>
                                       <children>
                                          <Text fx:id="text1" strokeType="OUTSIDE" strokeWidth="0.0" text="Restaurant1" />
                                       </children>
                                    </HBox>
                                 </children>
                                 <GridPane.margin>
                                    <Insets bottom="5.0" left="10.0" right="5.0" top="10.0" />
                                 </GridPane.margin>
                                 <padding>
                                    <Insets right="5.0" />
                                 </padding>
                              </VBox>
                           </children>
                        </GridPane>
                     </children>
                  </VBox>
                  <Button contentDisplay="CENTER" mnemonicParsing="false" onMouseClicked="#btnNext" style="-fx-background-radius: 25; -fx-min-width: 30; -fx-min-height: 30; -fx-max-width: 30; -fx-max-height: 30;" styleClass="gridpane" text="&gt;" textAlignment="CENTER">
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                     <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
            <HBox alignment="CENTER" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
               <padding>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
               </padding>
               <children>
                  <Text smooth="false" strokeLineCap="ROUND" strokeLineJoin="ROUND" strokeMiterLimit="0.0" style="-fx-font-weight: bolder; -fx-fill: white; -fx-font-size: 24;" text="ReservApp">
                     <effect>
                        <DropShadow />
                     </effect>
                  </Text>
                  <HBox alignment="CENTER_RIGHT" HBox.hgrow="ALWAYS" />
               </children>
            </HBox>
         </children>
      </AnchorPane>
   </children>
</VBox>
